<h1>Book detail</h1>
<div class="col-lg-8" id="main">
  <table class="table table-striped">
  <tr>
    <td><%= @book.title %></td>
    <td><%= @book.body %></td>
    <td><%= link_to "Edit","/books/#{@book.id}/edit" %></td>
    <td><%= link_to "destroy", book_path(@book.id),method: :delete, "data-confirm" => "本当に削除しますか？" %></td>
  </tr>
  </table>
</div>