<h1>Users</h1>
<div class="col-lg-8" id="main">
<table class="table table-striped">
  <th>image</th>
  <th>name</th>
  <th></th>
  <tr>
    <% @users.each do |user| %>
    <td><%= attachment_image_tag @user, :profile_image, :fill, 30, 30, format: 'jpeg' ,fallback:'no_image.jpg',size:'100x100'%></td>
    <td><%= user.name %></td>
    <td><%= link_to "show","/users/#{user.id}" %> </td>
  </tr>
  <% end %>
</table>